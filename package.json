{
  "name": "belshop",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "sh start.sh",
    "start-react": "react-native start",
    "pod-install": "rm -rf ios/Pods ios/build && cd ios && pod install && cd ..",
    "ios:cross": "cross-env-shell node ./src/process.js && react-native run-ios --simulator='iPhone 11' --scheme=\"Belshop$ENV\"",
    "ios": "cross-env ENV=dev node ./src/process.js && react-native run-ios --simulator='iPhone 11' --scheme=BelshopDev",
    "dev:ios": "cross-env ENV=Dev npm run ios:cross",
    "qa:ios": "cross-env ENV=QA npm run ios:cross",
    "staging:ios": "cross-env ENV=Staging npm run ios:cross",
    "prod:ios": "cross-env ENV=Prod node ./src/process.js && react-native run-ios --simulator='iPhone 11' --scheme=\"belshop\"",
    "build:ios:cross": "npm run pod-install && cross-env-shell BUILD=true PLATFORM=ios node ./src/process.js && xcodebuild -workspace ios/belshop.xcworkspace -scheme \"Belshop$ENV\" clean archive -configuration release -sdk iphoneos -archivePath \"ios/build/Belshop$ENV.xcarchive\" && xcodebuild -exportArchive -archivePath \"ios/build/Belshop$ENV.xcarchive\" -exportOptionsPlist ios/exportOptions.plist -exportPath \"ios/build/Belshop$ENV\"",
    "build:dev:ios": "cross-env ENV=Dev npm run build:ios:cross",
    "build:qa:ios": "cross-env ENV=QA npm run build:ios:cross",
    "build:staging:ios": "cross-env ENV=Staging npm run build:ios:cross",
    "build:prod:ios": "npm run pod-install && cross-env ENV=Prod BUILD=true PLATFORM=ios node ./src/process.js && xcodebuild -workspace ios/belshop.xcworkspace -scheme belshop clean archive -configuration release -sdk iphoneos -archivePath ios/build/belshop.xcarchive && xcodebuild -exportArchive -archivePath ios/build/belshop.xcarchive -exportOptionsPlist ios/exportOptions.plist -exportPath ios/build/Belshop",
    "android:cross": "cross-env-shell node ./src/process.js && react-native run-android --variant=\"$ENV\"Debug --appIdSuffix=$ENV",
    "android": "cross-env ENV=dev node ./src/process.js && react-native run-android --variant=DevDebug",
    "dev:android": "cross-env ENV=dev npm run android:cross",
    "qa:android": "cross-env ENV=qa npm run android:cross",
    "staging:android": "cross-env ENV=staging npm run android:cross",
    "prod:android": "cross-env ENV=Prod npm run android:cross",
    "bundle:prod:android": "cross-env-shell ENV=Prod BUILD=true PLATFORM=android node ./src/process.js && cd android && cross-env-shell ./gradlew bundle\"$ENV\"Release",
    "build:android:cross": "cross-env-shell BUILD=true PLATFORM=android node ./src/process.js && cd android && cross-env-shell ./gradlew assemble\"$ENV\"Release",
    "build:dev:android": "cross-env ENV=Dev npm run build:android:cross",
    "build:qa:android": "cross-env ENV=Qa npm run build:android:cross",
    "build:staging:android": "cross-env ENV=Staging npm run build:android:cross",
    "build:prod:android": "cross-env ENV=Prod npm run build:android:cross",
    "test": "jest",
    "lint:fix": "eslint --fix ./src",
    "lint": "eslint ./ --cache --ignore-pattern .gitignore",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.5.1",
    "@react-native-community/cli": "^3.0.4",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^4.4.0",
    "@react-native-firebase/analytics": "6.7.1",
    "@react-native-firebase/app": "6.7.1",
    "@react-native-firebase/auth": "6.7.1",
    "@react-native-firebase/crashlytics": "6.7.1",
    "@react-native-picker/picker": "^1.9.8",
    "@react-navigation/bottom-tabs": "^5.11.1",
    "@react-navigation/native": "^5.8.9",
    "@react-navigation/stack": "^5.12.6",
    "axios": "^0.18.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "form-data": "^2.5.1",
    "intl": "^1.2.5",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "patch-package": "^6.2.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-animatable": "^1.3.3",
    "react-native-biometrics": "^1.6.1",
    "react-native-branch": "^4.2.1",
    "react-native-cardview": "^2.0.5",
    "react-native-carousel-cards": "^1.2.1",
    "react-native-collapsible": "^1.5.3",
    "react-native-date-picker": "^3.2.5",
    "react-native-datepicker": "^1.7.2",
    "react-native-device-info": "^5.6.1",
    "react-native-dropdown-picker": "^3.7.1",
    "react-native-geocoding": "^0.3.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-image-picker": "^1.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-masked-text": "^1.13.0",
    "react-native-modal": "^11.5.6",
    "react-native-modalize": "^2.0.6",
    "react-native-navigation": "^2.7.0",
    "react-native-onesignal": "^3.7.2",
    "react-native-paper": "^4.4.0",
    "react-native-payments-reborn": "^0.7.9",
    "react-native-permissions": "^1.2.0",
    "react-native-picker-select": "^8.0.4",
    "react-native-progress": "^3.6.0",
    "react-native-push-notification": "git+https://git@github.com/zo0r/react-native-push-notification.git",
    "react-native-reanimated": "^1.13.2",
    "react-native-root-toast": "^3.0.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "2.11.0",
    "react-native-slick": "^1.6.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.1.1",
    "react-native-svg": "^9.13.6",
    "react-native-swipe-list-view": "^2.0.1",
    "react-native-swiper": "^1.6.0",
    "react-native-vector-icons": "^7.1.0",
    "react-native-webview": "^7.6.0",
    "react-navigation": "^3.11.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rn-placeholder": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.4",
    "fastlane": "^0.1.0",
    "jest": "^24.8.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.55.0",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "16.8.6"
  },
  "postinstall": "npx jetify",
  "jest": {
    "preset": "react-native"
  },
  "pre-commit": [
    "precommit-msg",
    "lint"
  ],
  "rnpm": {
    "assets": [
      "./assests/fonts/"
    ]
  }
}
